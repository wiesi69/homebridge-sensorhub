{
    "pluginAlias": "SensorHub",
    "pluginType": "platform",
    "singular": true,
    "schema": {
        "type": "object",
        "properties": {
            "name": {
                "description": "Plugin name as displayed in the homebridge log.",
                "type": "string",
                "default": "SensorHub"
              },
            "interval": {
                "description": "Sensor read interval in seconds.  Default: 10.",
                "type": "integer",
                "placeholder": 10,
                "minimum": 1,
                "maximum": 120
            },
            "disableTemperatureSensor": {
                "description": "Disnable temperature sensor.",
                "type": "boolean",
                "placeholder": false
            },
            "disableAirPressureSensor": {
                "description": "Disnable temperature sensor.",
                "type": "boolean",
                "placeholder": false
            },
            "disableLigthSensor": {
                "description": "Disnable ligth sensor.",
                "type": "boolean",
                "placeholder": false
            },
            "disableMotionDetectionSensor": {
                "description": "Disnable motion sensor.",
                "type": "boolean",
                "placeholder": false
            },
            "externalTempdisableExternalTemperatureSensoreratureSensor": {
                "description": "Disnable external temperatur sensor.",
                "type": "boolean",
                "placeholder": false
            },
            "temperaturCorrection": {
                "description": "Temperature value correction.  Default: -7.",
                "type": "integer",
                "placeholder": -7 
            },
            "brightnessCorrection": {
                "description": "Brightness value correction.  Default: 0.",
                "type": "integer",
                "placeholder": 0 
            },
            "externalTemperatureCorrection": {
                "description": "External temperature value correction.  Default: -3.",
                "type": "integer",
                "placeholder": -3 
            }
        }
    }
}